enum Role {
  Admin
  User
}

model Users {
  id         String   @id @default(uuid())
  email      String   @unique
  name       String
  surname    String
  password   String
  rating     Int      @default(0)
  role       Role     @default(User)
  created_at DateTime @default(now())

  from_user Transactions[] @relation("from")
  to_user Transactions[] @relation("to")
}
